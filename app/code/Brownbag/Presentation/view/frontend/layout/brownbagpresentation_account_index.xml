<?xml version="1.0"?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="1column" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account"/>
    <head>
        <title>Brownbag presentation tab</title>
    </head>
    <body>
    	<referenceBlock name="page.main.title">
    	    <action method="setPageTitle">
    	        <argument translate="true" name="title" xsi:type="string">BROWNBAG PRESENTATION</argument>
    	    </action>
    	</referenceBlock>
        <referenceContainer name="content">
            <block class="Magento\Framework\View\Element\Template" name="brownbagpresentation_account_index" template="Brownbag_Presentation::form.phtml"/>
        </referenceContainer>
    </body>
</page>
